{
  "name": "vue-command-menu",
  "version": "0.1.0",
  "description": "Build beautiful and extensible Command + k menus with Vue",
  "homepage": "https://github.com/BetaHuhn/vue-command-menu#readme",
  "keywords": [
    "vue",
    "typescript",
    "command palette",
    "command menu",
    "command k",
    "shortcuts"
  ],
  "author": "Maximilian Schiller <schiller@mxis.ch>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BetaHuhn/vue-command-menu.git"
  },
  "main": "dist/command.ssr.js",
  "browser": "dist/command.esm.js",
  "module": "dist/command.esm.js",
  "unpkg": "dist/command.min.js",
  "types": "command.d.ts",
  "files": [
    "dist/*",
    "command.d.ts"
  ],
  "scripts": {
    "dev": "vue-cli-service serve docs/index.ts",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js && cp lib/command.min.js docs/command.min.js && npm run build:web",
    "build:ssr": "cross-env NODE_ENV=production rollup --config rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config rollup.config.js --format iife",
    "build:web": "vue-cli-service build docs/index.ts"
  },
  "dependencies": {
    "fuse.js": "^6.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "lodash-es": "^4.17.20",
    "minimist": "^1.2.5",
    "rollup": "^2.37.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "typescript": "^3.9.7",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
